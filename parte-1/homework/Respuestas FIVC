-- Respuesta 1

select *
from stg.product_master
where categoria = 'Electro'

-- Respuesta 2

select *
from stg.product_master
where origen = 'China'

-- Respuesta 3

select *
from stg.product_master
where categoria = 'Electro'
order by nombre asc

-- Repsuesta 4

select *
from stg.product_master
where subcategoria = 'TV' and is_active = 'true'

-- Respuesta 5

select *
from stg.store_master
where pais = 'Argentina'
order by fecha_apertura asc

-- Respuesta 6

select * 
from stg.order_line_sale
order by fecha desc
limit 5

-- Respuesta 7

select * 
from stg.super_store_count
order by fecha asc
limit 10

-- Respuesta 8

select *
from stg.product_master
where subsubcategoria != 'Soporte' and subsubcategoria != 'Control remoto'

-- Respuesta 9

select *
from stg.order_line_sale
where venta > 100000 and moneda = 'ARS'

-- Respuesta 10

select *
from stg.order_line_sale
where fecha between '2022-10-01' and '2022-10-31'

-- respuesta 11

Select *
from stg.product_master
where ean is not null

-- Respuesta 12

Select *
from stg.order_line_sale
where fecha between '2022-10-01' and '2022-11-10'
and is_walkout = 'true'
